{
  "name": "scene-viewer-panels",
  "version": "1.0.0",
  "description": "panels for scene viewer",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" rimraf dist && node bin/esbuild.js && run-s -n emit-types",
    "emit-types": "tsc --emitDeclarationOnly --project tsconfig.json --outDir dist",
    "dev": "concurrently --names \"esbuild,tsc\" \"node bin/esbuild.watch.js\" \"tsc --emitDeclarationOnly --project tsconfig.json --outDir dist --watch\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "run-s -n eslint tsc",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "tsc": "tsc --noEmit --project tsconfig.json",
    "test": "run-s -n lint \"test:*\"",
    "test:build": "run-s -n build",
    "test:visual-regression": "build-storybook --quiet && loki test --reactUri file:./storybook-static --requireReference",
    "format": "prettier --write src/*",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
