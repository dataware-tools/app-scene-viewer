    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "inter-ui": "^3.0.0",
    "lint-staged": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-jsx": "^0.36.4",
