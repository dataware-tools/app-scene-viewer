{
  "name": "scene-viewer-panels",
  "version": "1.0.0",
  "description": "panels for scene viewer",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" rimraf dist && node bin/esbuild.js && run-s emit-types",
    "emit-types": " tsc --emitDeclarationOnly --project tsconfig.json --outDir dist",
    "test": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" run-s lint test:build",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" run-s eslint stylelint tsc",
    "lint:fix": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" run-s eslint:fix stylelint:fix format",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./src",
    "stylelint:fix": "stylelint ./ --fix",
    "tsc": "tsc --noEmit --project tsconfig.json",
    "test:build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" run-s build",
    "format": "prettier --write src/*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
