    "@types/react-dom": "^17.0.11",
    "@types/roslib": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.14",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-react": "^11.0.1",
